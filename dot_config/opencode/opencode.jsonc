{
  "$schema": "https://opencode.ai/config.json",
  "default_agent": "plan",
  "plugin": [
    "@franlol/opencode-md-table-formatter@latest",
    "opencode-pty@latest",
  ],
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}",
      },
    },
    "zai": {
      "type": "local",
      "command": ["bunx", "@z_ai/mcp-server@latest"],
      "environment": {
        "Z_AI_API_KEY": "{env:Z_AI_API_KEY}",
        "Z_AI_MODE": "ZAI",
      },
    },
    "chrome": {
      "type": "local",
      "command": ["bunx", "chrome-devtools-mcp@latest"],
    },
  },
