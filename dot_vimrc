call plug#begin()

Plug 'mechatroner/rainbow_csv'
Plug 'ayu-theme/ayu-vim'
Plug 'joshdick/onedark.vim'

call plug#end()

syntax on

set termguicolors
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
set clipboard=unnamed

inoremap kj <Esc>

#let ayucolor="mirage"
colorscheme onedark

#hi Normal guibg=NONE ctermbg=NONE
