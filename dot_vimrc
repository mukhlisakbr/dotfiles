call plug#begin()

Plug 'mechatroner/rainbow_csv'
Plug 'ayu-theme/ayu-vim'
Plug 'joshdick/onedark.vim'
Plug 'sheerun/vim-polyglot'
Plug 'rakr/vim-one'

call plug#end()

syntax on

set nocompatible
set termguicolors
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
set clipboard=unnamed

inoremap kj <Esc>

let ayucolor="mirage"
colorscheme one

hi Normal guibg=NONE ctermbg=NONE
