call plug#begin()

Plug 'mechatroner/rainbow_csv'
Plug 'ayu-theme/ayu-vim'

call plug#end()

set termguicolors
syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
inoremap kj <Esc>
set clipboard=unnamed
let ayucolor="dark"
colorscheme ayu
hi Normal guibg=NONE ctermbg=NONE
