call plug#begin()

Plug 'mechatroner/rainbow_csv'
Plug 'ayu-theme/ayu-vim'
Plug 'joshdick/onedark.vim'
Plug 'sheerun/vim-polyglot'
Plug 'rakr/vim-one'
Plug 'vv9k/vim-github-dark'

call plug#end()

syntax on

set nocompatible
set termguicolors
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
set clipboard=unnamed
" Use new regular expression engine
set re=0

inoremap kj <Esc>

let ayucolor="mirage"
let g:gh_color = "soft"
"colorscheme ghdark

"hi Normal guibg=NONE ctermbg=NONE

autocmd BufNewFile,BufRead .zshrc.tmpl set filetype=zsh
